{
  "name": "PrivateDiaryApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "release-android": "cd android && ./gradlew clean && cd .. && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew assembleRelease && cd ..",
    "adb-log": "adb logcat *:E",
    "run-release": "adb -s emulator-5554 install android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-google-signin/google-signin": "^7.2.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.4.0",
    "@robinbobin/react-native-google-drive-api-wrapper": "^1.2.1",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "crypto-es": "^2.1.0",
    "dayjs": "^1.11.0",
    "formik": "^2.2.9",
    "mobx": "^6.13.0",
    "mobx-react-lite": "^4.0.7",
    "mobx-state-tree": "^5.4.2",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-calendars": "^1.1305.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keychain": "^8.2.0",
    "react-native-reanimated": "^3.13.0",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.3.0",
    "realm": "12.3.0",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
